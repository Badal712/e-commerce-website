						<div class="row">
							<div class="input_box">
								<label class="lbl">Phone Number:</label>
								<input type="tel" name="telPhone" class="tbox" pattern="[0-9]{10}" required>
							</div>
							<div class="input_box">
								<label class="lbl">e-Mail :</label>
